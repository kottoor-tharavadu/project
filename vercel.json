{
    "builds":[
        {
            "src":"ecommerce/wsgi.py",
            "use":"@vercel/python",
            "config":{
                "maxLambadaSize":"50mb",
                "runtime":"python3.12"
            }
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"ecommerce/wsgi.py"
        }
    ]
}